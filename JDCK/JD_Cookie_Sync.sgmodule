#!name=JD Cookie Sync
#!desc=Intercepts JD cookies and syncs them to Qinglong Panel.
#!author=NongfuSpring
#!icon=https://raw.githubusercontent.com/Orz-3/mini/master/jd.png
#!homepage=https://github.com/
#!date=2024-05-18 10:10:10

[Script]
JD Cookie Sync = type=http-request,pattern=^https?:\/\/(?:api|wq|plogin)\.m\.jd\.com,requires-body=0,max-size=0,script-path=https://raw.githubusercontent.com/5jwoj/BeRich/main/JDCK/jd_cookie_sync_surge.js,script-update-interval=0,timeout=10,argument=ql_url=http://127.0.0.1:5700&ql_client_id=your_client_id&ql_client_secret=your_client_secret

[MITM]
hostname = %APPEND% api.m.jd.com plogin.m.jd.com wq.jd.com
